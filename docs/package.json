    "globby": "^16.1.0",
    "hast": "^1.0.0",
    "hast-util-heading-rank": "^3.0.0",
    "hast-util-to-string": "^3.0.1",
    "lucide-react": "^0.571.0",
    "lz-string": "^1.5.0",
    "match-sorter": "^8.2.0",
    "next": "16.1.6",
    "postcss": "^8.5.6",
