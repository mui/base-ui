{
  "extends": "../tsconfig.base.json",
  "include": [
    "app",
    "data",
    "pages",
    "src",
    "types",
    "next-env.d.ts",
    "next.config.mjs",
    ".next/types/**/*.ts",
    "export/types/**/*.ts"
  ],
  "compilerOptions": {
    "allowJs": true,
    "isolatedModules": true,
    /* files are emitted by babel */
    "noEmit": true,
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "types": [
      "react"
    ],
    "incremental": true,
    "composite": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true
  },
  "exclude": [
    "node_modules"
  ],
  "references": [
    {
      "path": "../packages/mui-base/tsconfig.build.json"
    }
  ]
}
