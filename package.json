{
  "name": "@base-ui/monorepo",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "deduplicate": "pnpm dedupe",
    "build": "lerna run --no-private build",
    "release:version": "lerna version --no-changelog --no-push --no-git-tag-version --no-private",
    "release:build": "lerna run --concurrency 8 --no-private build --skip-nx-cache",
    "release:changelog": "code-infra generate-changelog",
    "release:changelog:docs": "FORMAT=docs code-infra generate-changelog",
    "release:publish": "code-infra publish --github-release",
    "release:publish:dry-run": "code-infra publish --github-release --dry-run ",
    "docs:api": "pnpm --filter api-docs-builder start",
    "docs:build": "pnpm --filter docs generate-llms && pnpm --filter docs build && pnpm playground:build:export",
    "docs:deploy": "pnpm --filter docs run deploy",
    "docs:dev": "pnpm --filter docs dev",
    "docs:size-why": "cross-env DOCS_STATS_ENABLED=true pnpm docs:build",
    "docs:start": "pnpm --filter docs serve",
    "playground:build:export": "cross-env PLAYGROUND_BASE=/vite-playground/ PLAYGROUND_OUT_DIR=../../docs/export/vite-playground pnpm -C playground/vite-app build",
    "docs:link-check": "pnpm --filter docs link-check",
    "docs:generate-llms": "pnpm --filter docs run generate-llms",
    "docs:infra": "pnpm --filter docs run docs-infra",
    "docs:validate": "pnpm --filter docs run internal-validate --command 'pnpm docs:validate'",
    "extract-error-codes": "code-infra extract-error-codes --errorCodesPath docs/src/error-codes.json --detection opt-out",
    "eslint": "pnpm -r lint && eslint . --cache --report-unused-disable-directives --max-warnings 0",
    "eslint:ci": "pnpm -r lint && eslint . --report-unused-disable-directives --max-warnings 0",
    "stylelint": "stylelint --reportInvalidScopeDisables --reportNeedlessDisables \"docs/**/*.{js,ts,tsx}\" \"**/*.css\" --ignore-path .lintignore",
    "markdownlint": "markdownlint-cli2 \"**/*.{md,mdx}\"",
    "valelint": "pnpm dlx --package @vvago/vale vale sync && git ls-files | grep -E \"\\.(md|mdx)$\" | xargs pnpm dlx --package @vvago/vale vale --filter='.Level==\"error\"'",
    "prettier": "pretty-quick --ignore-path .lintignore --branch master",
    "prettier:all": "prettier --write . --ignore-path .lintignore",
    "size:snapshot": "pnpm -F ./test/bundle-size check",
    "size:why": "pnpm size:snapshot --analyze",
    "start": "pnpm install && pnpm docs:dev",
    "test": "pnpm test:chromium",
    "test:e2e": "cross-env NODE_ENV=production pnpm test:e2e:build && concurrently --success first --kill-others \"pnpm test:e2e:run\" \"pnpm test:e2e:server\"",
    "test:e2e:build": "vite build --config test/e2e/vite.config.mjs",
    "test:e2e:dev": "vite --config test/e2e/vite.config.mjs -l info --port 5173",
    "test:e2e:run": "cross-env VITEST_ENV=chromium vitest run --project e2e",
    "test:e2e:server": "serve test/e2e -p 5173",
    "test:regressions": "cross-env NODE_ENV=production pnpm test:regressions:build && concurrently --success first --kill-others \"pnpm test:regressions:run\" \"pnpm test:regressions:server\"",
    "test:regressions:build": "vite build --config test/regressions/vite.config.mjs",
    "test:regressions:dev": "vite --config test/regressions/vite.config.mjs --port 5173",
    "test:regressions:run": "cross-env VITEST_ENV=chromium vitest run --project regressions",
    "test:regressions:server": "serve test/regressions -p 5173",
    "test:_unit": "cross-env TZ=UTC vitest --project @base-ui/react --project @base-ui/utils --project docs",
    "test:jsdom": "cross-env VITEST_ENV=jsdom pnpm test:_unit",
    "test:jsdom:coverage": "pnpm test:jsdom --coverage",
    "test:chromium": "cross-env VITEST_ENV=chromium pnpm test:_unit",
    "test:chromium:ui": "pnpm test:chromium --browser.headless=false",
    "test:firefox": "cross-env VITEST_ENV=firefox pnpm test:_unit",
    "test:firefox:ui": "pnpm test:firefox --browser.headless=false",
    "test:webkit": "cross-env VITEST_ENV=webkit pnpm test:_unit",
    "test:webkit:ui": "pnpm test:webkit --browser.headless=false",
    "test:browsers": "cross-env VITEST_ENV=all-browsers pnpm test:_unit",
    "test:argos": "code-infra argos-push --folder test/regressions/screenshots/chrome",
    "typescript": "tsc -b tsconfig.json",
    "inline-scripts": "tsx ./scripts/inlineScripts.mts",
    "use-react-18": "code-infra set-version-overrides --pkg react@18"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.18.2",
    "@babel/plugin-transform-react-constant-elements": "7.27.1",
    "@base-ui/monorepo-tests": "workspace:*",
    "@mui/internal-code-infra": "0.0.3-canary.94",
    "@mui/internal-netlify-cache": "0.0.3-canary.0",
    "@mui/internal-test-utils": "2.0.18-canary.8",
    "@next/eslint-plugin-next": "16.1.6",
    "@octokit/rest": "22.0.1",
    "@playwright/test": "1.58.2",
    "@tailwindcss/postcss": "4.1.18",
    "@types/node": "22.18.13",
    "@types/react": "19.2.14",
    "@types/yargs": "17.0.35",
    "@vitejs/plugin-react": "5.1.4",
    "@vitest/browser-playwright": "4.0.18",
    "@vitest/coverage-istanbul": "4.0.18",
    "@vitest/ui": "4.0.18",
    "chai-dom": "1.12.1",
    "concurrently": "9.2.1",
    "cross-env": "10.1.0",
    "docs": "workspace:^",
    "eslint": "9.39.2",
    "execa": "9.6.1",
    "globby": "16.1.0",
    "lerna": "9.0.4",
    "markdownlint-cli2": "0.21.0",
    "prettier": "3.8.1",
    "pretty-quick": "4.2.2",
    "publint": "0.3.17",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "remark-mdx": "3.1.1",
    "rimraf": "6.1.3",
    "serve": "14.2.5",
    "stylelint": "17.3.0",
    "stylelint-config-tailwindcss": "1.0.1",
    "tailwindcss": "4.1.18",
    "terser": "5.46.0",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vitest": "4.0.18"
  },
  "packageManager": "pnpm@10.29.3",
  "engines": {
    "pnpm": "10.29.3",
    "node": ">=22.18.0"
  },
  "resolutions": {
    "@babel/core": "^7.29.0",
    "@babel/plugin-transform-runtime": "^7.29.0",
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@babel/types": "^7.29.0",
    "@types/node": "22.18.13",
    "@types/react": "19.2.14",
    "@types/react-dom": "^19.2.3"
  }
}
